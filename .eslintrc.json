{
    "parser": "babel-eslint",
    "extends": ["airbnb", "prettier"],
    "rules": {
        "import/prefer-default-export": false,
        "indent": ["error", 4],
        "import/no-extraneous-dependencies": false,
        "react/jsx-indent": ["error", 4],
        "react/jsx-indent-props": ["error", 4],
        "react/prop-types": false
    },
    "env": {
        "browser": true,
        "node": true
    }
}
